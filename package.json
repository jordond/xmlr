{
  "name": "xmlr",
  "productName": "xmlR",
  "version": "0.1.0",
  "main": "./dist/main.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-register --recursive --require ./test/setup.js test/**/*.spec.js",
    "test-watch": "npm test -- --watch",
    "test-e2e": "cross-env NODE_ENV=test mocha --compilers js:babel-register --require ./test/setup.js --require co-mocha ./test/e2e.js",
    "lint": "eslint src test *.js",
    "hot-server": "node -r babel-register server.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config ./webpack/config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config ./webpack/config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron ./",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./src/main",
    "package": "cross-env NODE_ENV=production node -r babel-register package.js",
    "package-all": "npm run package -- --all",
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\""
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chentsulin/xmlr.git"
  },
  "author": {
    "name": "Jordon de Hoog",
    "email": "jordon@hoogit.ca",
    "url": "https://github.com/jordond"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jordond/xmlr/issues"
  },
  "devDependencies": {
    "asar": "^0.12.3",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-remove-console": "^6.8.0",
    "babel-plugin-transform-remove-debugger": "^6.8.0",
    "babel-plugin-webpack-loaders": "^0.8.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-register": "^6.16.3",
    "chai": "^3.5.0",
    "co-mocha": "^1.1.3",
    "concurrently": "^3.1.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "del": "^2.2.2",
    "devtron": "^1.3.0",
    "electron-devtools-installer": "^2.0.1",
    "electron-packager": "^8.1.0",
    "electron-prebuilt": "^1.4.2",
    "electron-rebuild": "^1.2.1",
    "eslint": "^3.7.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-import-resolver-webpack": "^0.6.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fbjs-scripts": "^0.7.1",
    "jsdom": "^9.6.0",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "mobx-react-devtools": "^4.2.6",
    "mocha": "^3.1.0",
    "node-libs-browser": "^1.0.0",
    "react-addons-test-utils": "^15.3.2",
    "sinon": "^1.17.6",
    "spectron": "^3.4.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.3",
    "webpack-hot-middleware": "^2.12.2"
  },
  "dependencies": {
    "css-modules-require-hook": "^4.0.5",
    "electron-debug": "^1.0.1",
    "font-awesome": "^4.6.3",
    "mobx": "^2.5.2",
    "mobx-react": "^3.5.7",
    "node-uuid": "^1.4.7",
    "postcss": "^5.2.4",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^2.8.1",
    "source-map-support": "^0.4.3"
  },
  "devEngines": {
    "node": "4.x || 5.x || 6.x",
    "npm": "2.x || 3.x"
  }
}
